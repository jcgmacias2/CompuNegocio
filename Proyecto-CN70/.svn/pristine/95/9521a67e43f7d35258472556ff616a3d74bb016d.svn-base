<base:BaseView x:Class="Aprovi.Views.UI.AddendaJardinesView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:base="clr-namespace:Aprovi.Views"
        xmlns:hlp="clr-namespace:Aprovi.Application.Helpers"
        Title="Addenda Jardines"
        Icon="{StaticResource ImgCatalogs}"
        WindowStartupLocation="CenterScreen"
        Style="{StaticResource WindowBorder}"
        FocusManager.FocusedElement="{Binding ElementName=txtNumeroRM}"
        Height="170" Width="320">
    <Grid Background="{StaticResource GrayBrownToWhiteGradient}" hlp:EnterKeyTraversal.IsEnabled="True">
        <Label Style="{StaticResource LabelSmall}" Content="Número RM :" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtNumeroRM" HorizontalAlignment="Left" Height="23" Margin="91,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199"/>
        <Button x:Name="btnCerrar" Content="Guardar" Style="{StaticResource BlockButton}" Margin="130,60,0,0" Tag="{StaticResource ImgSave}"/>
    </Grid>
    </base:BaseView>



