using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Aprovi.Business.ViewModels
{
    public class VMCredencial
    {
        public string Usuario { get; set; }
        public string Contraseña { get; set; }
        public bool APIAuthorized { get; set; }

        public VMCredencial(string username, string password, bool apiAuthorized)
        {
            Usuario = username;
            Contraseña = password;
            APIAuthorized = apiAuthorized;
        }
    }
}
