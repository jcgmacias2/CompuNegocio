<base:BaseView x:Class="Aprovi.Views.UI.AddendaComercialMexicanaView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:base="clr-namespace:Aprovi.Views"
        xmlns:hlp="clr-namespace:Aprovi.Application.Helpers"
        Title="Addenda Comercial Mexicana"
        Icon="{StaticResource ImgCatalogs}"
        WindowStartupLocation="CenterScreen"
        Style="{StaticResource WindowBorder}"
        FocusManager.FocusedElement="{Binding ElementName=txtCuenta}"
        Height="230" Width="320">
    <Grid Background="{StaticResource GrayBrownToWhiteGradient}" hlp:EnterKeyTraversal.IsEnabled="True">
        <Label Style="{StaticResource LabelSmall}" Content="Fecha de entrega :" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="dpFechaEntrega" HorizontalAlignment="Left" Height="23" Margin="117,20,0,0" VerticalAlignment="Top" Width="173"/>
        <Label Style="{StaticResource LabelSmall}" Content="Sucursal :" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbSucursales" HorizontalAlignment="Left" Height="23" Margin="117,50,0,0" VerticalAlignment="Top" Width="173"/>
        <Label Style="{StaticResource LabelSmall}" Content="Sección :" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbSecciones" HorizontalAlignment="Left" Height="23" Margin="117,80,0,0" VerticalAlignment="Top" Width="173"/>
        <Button x:Name="btnCerrar" Content="Guardar" Style="{StaticResource BlockButton}" Margin="130,120,0,0" Tag="{StaticResource ImgSave}"/>
    </Grid>
</base:BaseView>



